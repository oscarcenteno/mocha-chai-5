{
    "compilerOptions": {
      "outDir": "./lib",
      "sourceMap": true,
      "module": ""
      "moduleResolution": "NodeNext",
      "target": "esnext",
      "forceConsistentCasingInFileNames": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noFallthroughCasesInSwitch": true,
      "strict": true
    },
    "include": [
      "src/**/*.ts"
    ]
  }